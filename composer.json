{
  "name": "react-inspector/prometheus-printer",
  "description": "\ud83d\udcdc Metrics Printer in Prometheus format",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com",
      "homepage": "http://wyrihaximus.net/"
    }
  ],
  "require": {
    "php": "^7.4",
    "react-inspector/metric": "^1.0",
    "react-inspector/metrics": "^1.0",
    "react-inspector/printer": "^1.0"
  },
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^4.0.6"
  },
  "config": {
    "platform": {
      "php": "7.4.7"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ReactInspector\\Printer\\Prometheus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReactInspector\\Tests\\Printer\\Prometheus\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
